{
  "name": "prettier-plugin-jinja-json-template",
  "version": "0.0.2",
  "description": "Prettier plugin for formatting jinja json templates.",
  "author": "David Odenwald, Bystray Stepan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeoRu4/prettier-plugin-jinja-json-template.git"
  },
  "bugs": {
    "url": "https://github.com/NeoRu4/prettier-plugin-jinja-json-template/issues"
  },
  "homepage": "https://github.com/NeoRu4/prettier-plugin-jinja-json-template#readme",
  "keywords": [
    "prettier",
    "plugin",
    "template",
    "json",
    "jinja",
    "jinja2",
    "flask"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    "require": "./index.cjs.js",

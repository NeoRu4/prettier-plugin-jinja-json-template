    "publish": "node --es-module-specifier-resolution=node ./scripts/publish.mjs"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.7.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/eslint": "^9",
    "@types/jest": "^29.5.12",
    "@types/semver": "^7",
    "eslint": "9.x",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.3.3",
    "rollup": "^4.19.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "semver": "^7.6.3",
    "ts-jest": "^29.1.4",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.17.0"
  },
